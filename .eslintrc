{
    "rules": {
        "indent" : [ 2, "tab" ],
        "quotes" : [ 2, "single" ],
        "linebreak-style" : [ 2, "unix" ],
        "semi" : [ 2, "always" ],
        "strict" : [2, "never"],
        "comma-dangle" : [2, "never"],
        "no-empty" : 2,
        "no-extra-semi" : 2,
        "no-unreachable" : 2,
        //"complexity" : [2, 2],
        "consistent-return" : 2,
        "eqeqeq" : 2,
        "guard-for-in" : 2,
        "no-else-return" : 1,
        "no-eq-null" : 2,
        "no-extra-bind" : 2,
        "no-invalid-this" : 2,
        "no-native-reassign" : 2,
        "no-redeclare" : 2,
        "no-return-assign" : 2,
        "no-self-compare" : 2,
        "no-useless-concat" : 2,
        //"vars-on-top" : 0,
        "yoda" : [2, "never"],
        "brace-style" : 1,
        "camelcase" : [1, {"properties": "never"}],
        "comma-spacing" : [2, {"before": false, "after": true}],
        "jsx-quotes" : [2, "prefer-double"],
        "no-spaced-func" : 2,
        //"one-var": [2, "always"]
        "semi-spacing" : [2, {"before": false, "after": true}],
        //"space-after-keywords": [2, "never"]
        "space-before-blocks" : [2, "never"],
        "space-infix-ops" : 2,
        "arrow-spacing" : [2, { "before": true, "after": true }],
        //"prefer-template" : 1,
        "prefer-const" : 1
    },
    "env": {
        "es6" : true,
        "browser" : true
    },
    "extends" : "eslint:recommended",
    "ecmaFeatures" : {
        "jsx" : true,
        "experimentalObjectRestSpread" : true,
        "modules" : true
    },
    "plugins" : [ "react" ]
}